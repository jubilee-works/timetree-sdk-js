{
  "name": "@timetreeapp/web-api",
  "version": "3.0.1",
  "description": "Official SDK for using TimeTree WebAPI",
  "author": "TimeTree, Inc. <dev-platform@timetreeapp.com>",
  "repository": "https://github.com/jubilee-works/timetree-sdk-js",
  "homepage": "https://github.com/jubilee-works/timetree-sdk-js",
  "keywords": [
    "timetree",
    "web-api",
    "client",
    "http",
    "api"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/browser.js",
  "license": "MIT",
  "scripts": {
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint --cache './**/*.{js,ts}'",
    "lint": "run-p lint:*",
    "test": "jest",
    "release": "semantic-release",
    "build:node": "rollup -c ./rollup.config.js",
    "build:browser": "TARGET=browser rollup -c ./rollup.config.js",
    "clean": "rimraf dist",
    "prepublishOnly": "run-s clean build:node build:browser"
  },
  "dependencies": {
    "async-retry": "^1.3.1",
    "axios": "^0.21.0",
    "humps": "^2.0.1",
    "jsonwebtoken": "^9.0.0",
    "kitsu-core": "^8.0.4",
    "pluralize": "^8.0.0",
    "qs": "^6.9.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-replace": "3.0.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/async-retry": "1.4.3",
    "@types/humps": "2.0.1",
    "@types/jest": "27.0.3",
    "@types/jsonwebtoken": "8.5.6",
    "@types/pluralize": "0.0.29",
    "@types/qs": "6.9.7",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-functional": "3.7.2",
    "jest": "27.5.1",
    "nock": "13.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "rimraf": "3.0.2",
    "rollup": "2.73.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "semantic-release": "19.0.5",
    "ts-jest": "27.1.5",
    "typescript": "4.9.5"
  },
  "resolutions": {
    "semantic-release/**/yargs-parser": ">=13.1.2",
    "semantic-release/**/mem": ">=4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
